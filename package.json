{
  "devDependencies": {
    "concurrently": "^9.1.2"
  },
  "scripts": {
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\" \"npm run dev:electron\"",
    "dev:frontend": "cd frontend && npm run dev",
    "dev:backend": "cd backend && uvicorn main:app --reload",
    "dev:electron": "cd electron && npm start"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.0.10",
    "react-icons": "^5.5.0",
    "tailwindcss": "^4.0.10"
  }
}
